{
  "name": "angular-email-editor-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-lib": "ng build @fractalframing/angular-email-editor --configuration production",
    "npm-pack": "cd dist/@fractalframing/angular-email-editor && npm pack",
    "copy-readme": "@powershell copy README.md dist/@fractalframing/angular-email-editor",
    "package": "npm run build-lib && npm run copy-readme && npm run npm-pack",
    "release": "npm run package && cd dist/@fractalframing/angular-email-editor && npm publish --access=public"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^12.2.15",
